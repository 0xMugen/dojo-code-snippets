{
  "name": "cairo-dojo-snippets",
  "displayName": "Cairo Dojo Snippets",
  "description": "Snippets for Cairo Dojo development",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.60.0"
  },
  "categories": [
      "Snippets"
  ],
  "activationEvents": [
      "onLanguage:cairo"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "languages": [
          {
              "id": "cairo",
              "extensions": [
                  ".cairo"
              ],
              "aliases": [
                  "Cairo",
                  "cairo"
              ]
          }
      ]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
      "@types/node": "^14.14.37",
      "@types/vscode": "^1.60.0",
      "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xMugen/dojo-code-snippets"
    },
    "publisher": "0xMugen",
    "license": "MIT"
}